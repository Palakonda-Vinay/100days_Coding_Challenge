import java.util.*;
public class Main
{
	public static void main(String[] args) // Reverse the Array
	{
	    int A[]={1,2,3,4,5,6,7};
	    int temp=0;
	    int i=0;
	    int j=A.length-1;
	    while(i<j)
	    {
	        temp=A[i];
	        A[i]=A[j];
	        A[j]=temp;
	        i++;
	        j--;
	    }
	    for(int k=0;k<A.length;k++)
	    {
	        System.out.print(A[k]+" ");
	    }
	}
}
